    "rollup": "*",
    "@rollup/plugin-node-resolve": "*",
    "rollup-plugin-delete": "*",
    "rollup-plugin-svelte": "*",
    "rollup-plugin-scss": "*",
    "rollup-plugin-terser": "*",
    "rollup-plugin-sizes": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/parakin/cfc-server.git"
